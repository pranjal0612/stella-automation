		<listener class-name="com.stella.playwright.listener.StellaListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
	</listeners>
	<test name="SignupAllTest">
		<parameter name="sheetName" value="sign-up"></parameter>
		<classes>
			<class name="com.stella.playwright.test.SignEmailValidationTest" />
			<class name="com.stella.playwright.test.SignFailedTest" />
